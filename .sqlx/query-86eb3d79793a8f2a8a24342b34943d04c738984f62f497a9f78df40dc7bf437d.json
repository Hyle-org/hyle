  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            t.tx_hash,\n            t.block_hash,\n            t.tx_index,\n            t.version,\n            t.transaction_type as \"transaction_type: TransactionType\",\n            t.transaction_status as \"transaction_status: TransactionStatus\"\n        FROM transactions t\n        JOIN blocks b ON t.block_hash = b.hash\n        WHERE b.height = $1\n        ",
